{
    "$schema": "https://raw.githubusercontent.com/Unkn0wnCat/keyindex/main/data/schemas/protocols.schema.json",
    "id": "vks",
    "name": "Verifying Keyserver",
    "spec": "https://keys.openpgp.org/about/api",
    "endpoints": [
        {
            "url": "/vks/v1/by-email/{{email}}",
            "type": "by-email"
        },
        {
            "url": "/vks/v1/by-keyid/{{keyid}}",
            "type": "by-keyid"
        },
        {
            "url": "/vks/v1/by-fingerprint/{{fingerprint}}",
            "type": "by-fingerprint"
        }
    ]
}